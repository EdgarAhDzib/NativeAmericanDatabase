<div class="Background"></div>
<nav class="navbar navbar-inverse  ">
	<div class="container-fluid">
		<div class="navbar-header">
			<a class="navbar-brand" href="../">The Native American Database</a>
		</div>
		<ul class="nav navbar-nav navbar-right">
			<li><a href="#"><span class="glyphicon glyphicon-user"></span> Sign Up</a></li>
			<li><a href="#"><span class="glyphicon glyphicon-log-in"></span> Login</a></li>
		</ul>
	</div>
</nav>
<div class="container">
	<h1 class="text-center">The Native American Database</h1>
	<div class="circular-menu">
		<div class="circle">
			<section>
				<div class="container">
					<div class="row">
						<div class="span4"></div>
						<div id="mask" class="span4">
							<img class="center-block" src="../assets/images/kwakiutl-clan-mask.png" class="img-responsive" alt="Kwakiutl Clan Mask" style="width:200px;height:200px" ;></div>
						<div class=" span4 "></div>
					</div>
				</div>
		</div>
	</div>

{{#if isGroup}}
<h2>Results for
	{{#each entry}}
		{{#if @first}}
			{{this.group}}
		{{/if}}
	{{/each}}
</h2>
{{/if}}

{{#if isSubj}}
<h2>Results for 
	{{#each entry}}
		{{#if @first}}
			{{#each content_fields}}
				'{{this.ethn_id}}'
			{{/each}}
		{{/if}}
	{{/each}}
</h2>
{{/if}}

{{#if searchParam}}

	{{> search/search}}

{{else}}

{{/if}}