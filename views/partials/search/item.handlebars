{{#each entry}}
	<div>
		<h1 class="text-center" style="font-weight:700">{{{this.item_title}}}</h1>
		<h2 class="text-center">{{this.group}}</h2>
		
		{{#each media_sources}}
			{{#if @first}}
				<h3 class="text-center">Institution: {{this.museum}}</h3>
			{{/if}}

			<div class="youTubeFull" id="{{this.youtube}}"></div>

		{{/each}}
		
			<div class="col-xs-12">
				<h4 class="text-center">Keywords: 
					{{#each content_fields}}
						<span><span style="font-size:150%">Â·</span> <a href="../../subj/{{this.ethn_id}}">{{this.ethn_id}}</a> </span>
					{{/each}}
				</h4>
			</div>

		{{#each media_sources}}
			{{#if @first}}
				<div class="col-md-6">
					<img class="imgOne" src="{{this.img_ref_1}}" width="350"/>
				</div>
			{{/if}}
				<div class="col-md-6">
					<img class="imgTwo" src="{{this.img_ref_2}}" width="350"/>
				</div>

			<div class="col-md-6">
				<img class="imgThree" src="{{this.img_ref_3}}" width="350"/>
			</div>
			<div class="col-md-6">
				<img class="imgFour" src="{{this.img_ref_4}}" width="350"/>
			</div>
		{{/each}}

		<div class="col-xs-12">
			<h5>
				<a href="{{source_ref.url}}" target="_blank">Link to original source</a>
			</h5>
		</div>
		
	</div>
{{/each}}